{
  "name": "nuxt-rollbar",
  "type": "module",
  "version": "0.2.2",
  "packageManager": "pnpm@8.7.4",
  "description": "Rollbar module for Nuxt 3",
  "author": "Jairo Blatt <jairoblattc@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/jairoblatt/nuxt-rollbar#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jairoblatt/nuxt-rollbar.git"
  },
  "bugs": "https://github.com/jairoblatt/nuxt-rollbar/issues",
  "keywords": [
    "nuxt",
    "nuxt3",
    "rollbar",
    "tracking",
    "logging",
    "debugging",
    "error"
  ],
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "require": "./dist/module.cjs",
      "import": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test:types": "tsc --noEmit",
    "format": "prettier \"**/*.{css,html,json,md,mjs,ts,vue,yml}\" --write",
    "format:check": "prettier \"**/*.{css,html,json,md,mjs,ts,vue,yml}\" --check",
    "release": "bumpp --commit --push --tag",
    "prepare": "nuxi prepare playground"
  },
  "dependencies": {
    "@nuxt/kit": "^3.7.1",
    "consola": "^3.2.3",
    "defu": "^6.1.2",
    "pathe": "^1.1.1",
    "rollbar": "^2.26.2"
  },
  "devDependencies": {
    "@nuxt/eslint-config": "^0.2.0",
    "@nuxt/module-builder": "^0.5.1",
    "@nuxt/schema": "^3.7.1",
    "@types/node": "^20.5.9",
    "bumpp": "^9.2.0",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "nuxt": "^3.7.1",
    "nuxt-rollbar": "workspace:*",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2",
    "vue-tsc": "^1.8.10"
  }
}
